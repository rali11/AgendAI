<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="App\Appointment\AvailableAppointment\Domain\AvailableAppointment" table="available_appointment">
        <id name="id" type="guid" column="id">
        </id>
        <field name="date" type="datetime" column="date_time" />
        <field name="durationInMinutes" type="integer" column="duration_minutes" />
        <field name="isActive" type="boolean" column="is_active" />
    </entity>
</doctrine-mapping>